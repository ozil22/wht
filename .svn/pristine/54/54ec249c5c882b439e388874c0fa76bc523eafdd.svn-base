
.Center{width: 96%;margin: 0 auto;background: #F6F6F6;
    position: relative;}
.p2{margin-bottom: 1rem;font-size: 1.8rem;}
.sp2{padding-right: 1rem;}
.sp3{font-size: 1.6rem;color: #7D7D7D;}
.bot{width: 100%;height:100px;position: fixed;bottom: 0px;border-top:1px solid #E5E5E5;background: #fff;}
.bott{width: 96%;height:80px;margin: 0 auto;padding: 10px 0;
      background: #fff;}
.bott_content{float: left;height:80px;width: 30%;}
 .img1{float: left;padding-right: 5%;}
 .p1{font-size: 1.6rem;padding:1rem 0;color: #7D7D7D;width: 150%;overflow: hidden;
      white-space:nowrap;/*强制文本在一行内显示*/
      text-overflow:ellipsis; /*溢出省略号，支持ie、safari（webkit）*/
    -o-text-overflow:ellipsis; /*溢出省略号，支持opera*/}
 .sp1{font-family: arial;font-size: 1.6rem;color: #373737;}
 .p3{text-align: center;border-top:1px dashed #7D7D7D;font-size: 1.6rem;
 color: #7D7D7D;padding: 2rem 0;cursor: pointer;}
 .btn{border: 0px;height: 50px;background: #FF5F28;color: #fff;width: 26%;text-align: center;line-height: 50px;
       font-size: 1.8rem;float: right;padding: 0 1rem;margin-top: 1.5rem;}
.z_header{width: 20%;position:relative;
        border-radius: 100%;
      -moz-border-radius: 100%;
    -webkit-border-radius: 100%;
    box-shadow: 0 0 0 0.2rem rgba(0,0,0,0.2);
}
.z_transparency{width:100%;background: rgba(114,112,107,.5);z-index:2;position: fixed;top: 0px;display: none;}
.z_typeChoose{display:none;width: 100%;background:#fff;position: fixed;left: 0;z-index: 2;}
.z_bottomBoxImg{width: 10%;margin: 0.5rem;float: left;min-height: 35px;}
.z_typeChooseCenter{width: 70%;margin: 0.5rem 0;float: left;}
.z_bottomBoxCenterTop,.z_bottomBoxCenterButton,.z_orderInfoBoxCenterTop,.z_orderInfoBoxCenterBottom{
    font-size: 1.2rem;color: #808080;
    white-space:nowrap;/*强制文本在一行内显示*/
    text-overflow:ellipsis; /*溢出省略号，支持ie、safari（webkit）*/
    -o-text-overflow:ellipsis; /*溢出省略号，支持opera*/
    overflow:hidden;/*溢出隐藏*/
    -moz-binding:url('ellipsis.xml#ellipsis');/*溢出省略号，支持firefox*/
}


.z_typeChooseBtnClose{font-size: 1.5rem;color: #808080;text-align: center}
.z_typeChooseBtn{display:inline-block;padding: 0.2rem 1rem;margin:0.2rem;border: 1px solid #ccc;font-size: 1.2rem;}
.z_typeChooseBtnSize{border: 0;padding: 0;}
.z_typeChooseBtnSbu{float: left;border: 1px solid #ccc;padding: 0.2rem 1rem;}
.z_typeChooseBtnAdd{float: right;border: 1px solid #ccc;padding: 0.2rem 1rem;}
.z_typeChooseBtnNumber{float: left;width:4rem;padding: 0.2rem 0;border: 0px;text-align: center;}
.z_ware{float: left;font-size: 1.2rem;padding-left: 0.5rem}
.z_typeChooseSub{padding: 0.4rem 0;background: #fb3c10;text-align: center;color: #fff;font-size: 1.4rem;
    border-radius: 0.2rem;-moz-border-radius: 0.2rem;-webkit-border-radius: 0.2rem}
/*下单*/
.z_addAddresBtn{width: 90%;margin:0 auto;font-size:1.2rem;text-align:center;color:#fb3c10;border: 1px solid #fb3c10;padding: 0.5rem 0}
.z_orderInfoBoxImg{width: 15%;margin: 0.5rem;float: left}
.z_orderInfoBoxCenter{width: 40%;margin: 0.5rem 0;float: left}
.z_orderInfoBoxRight{float: right;margin: 0.5rem 2rem 0.5rem 0;/*border: 1px solid #CBCBCB;*/font-size: 1.2rem;}
.z_orderInfoBoxRightBtnSbu{border-right: 2px solid #ccc;float: left;padding: 0.3rem 0.7rem;font-size: 1.2rem;}
.z_orderInfoBoxRightBtnNumber{padding: 0.3rem 0;font-size: 1.2rem;width:3rem;line-height:100%;float: left;font-size: 1.2rem;text-align: center;}
.z_orderInfoBoxRightBtnAdd{border-left: 2px solid #ccc;float: left;padding: 0.3rem 0.7rem;font-size: 1.2rem}
.z_coupon{padding: 0.5rem 1rem;}
.z_addressBox{position: absolute;top: 0;left:0;z-index: 5}
.z_xiangzuo{float: left;width: 0.7rem;padding: 1rem;position: absolute;left: 0;top: 0}
.z_resuleName,.z_resuleTel,.z_resuleAddre,.z_resuleMoreAddre{border: none;padding: 0.6rem 0}
.z_addressBoxS{width: 98%;margin:auto;text-align:center;background: #fb3c10;color: #fff;padding: 0.8rem 0;font-size: 1.6rem}
.z_resuleAddre{background: #fb3c10;color: #fff}
.Conte{width: 100%;background: #fff;display: none;}
.kucun{float: left;padding: 0.2rem 1rem 0.2rem 1rem; font-size: 1.2rem;color: #7D7D7D;}

